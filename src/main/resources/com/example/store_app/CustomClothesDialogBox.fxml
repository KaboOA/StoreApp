    <content>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                  prefHeight="400.0" prefWidth="420.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0" prefWidth="117.99999237060544"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="462.00000762939453" minWidth="10.0" prefWidth="199.2"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="462.00000762939453" minWidth="10.0" prefWidth="106.0"/>

            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>

            </rowConstraints>
            <Text text="Name">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <Text text="Price" GridPane.rowIndex="2">

            </Text>
            <Text text="Image" GridPane.rowIndex="3">

            </Text>
            <TextField fx:id="clothesNameTextField" GridPane.columnIndex="1"/>
            <TextField fx:id="clothesPriceTextField" prefHeight="26.0" prefWidth="169.0" GridPane.columnIndex="1"
                       GridPane.rowIndex="2"/>
            <TextField fx:id="clothesImageTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <Button mnemonicParsing="false" onAction="#clothesChooseImageBtnClicked" text="..." GridPane.columnIndex="2"
                    GridPane.rowIndex="3">
                <GridPane.margin>
                    <Insets left="20.0"/>
                </GridPane.margin>
            </Button>
            <Text text="Quantity" GridPane.rowIndex="4">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <TextField fx:id="clothesQuantityTextField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            <TextField fx:id="clothesManifacturerTextField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            <TextField fx:id="clothesYOPTextField" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
            <TextField fx:id="clothesTypeTextField" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
            <Text text="Supplier" GridPane.rowIndex="5">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <Text text="Year of Production" wrappingWidth="115.0" GridPane.rowIndex="6">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <Text text="Type" GridPane.rowIndex="7">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <Text text="Sizes" GridPane.rowIndex="8">
                <GridPane.margin>
                    <Insets/>
                </GridPane.margin>
            </Text>
            <CheckBox mnemonicParsing="false" text="XL" GridPane.columnIndex="1" GridPane.rowIndex="8"
                      fx:id="xlCheckboxId"/>
            <CheckBox fx:id="lCheckboxId" mnemonicParsing="false" text="L" GridPane.columnIndex="1"
                      GridPane.rowIndex="8">
                <GridPane.margin>
                    <Insets left="45.0"/>
                </GridPane.margin>
            </CheckBox>
            <CheckBox fx:id="mCheckboxId" mnemonicParsing="false" text="M" GridPane.columnIndex="1"
                      GridPane.rowIndex="8">
                <GridPane.margin>
                    <Insets left="85.0"/>
                </GridPane.margin>
            </CheckBox>
            <CheckBox fx:id="sCheckboxId" mnemonicParsing="false" text="S" GridPane.columnIndex="1"
                      GridPane.rowIndex="8">
                <GridPane.margin>
                    <Insets left="135.0"/>
                </GridPane.margin>
            </CheckBox>
            <HBox alignment="BOTTOM_CENTER" GridPane.columnIndex="1" GridPane.rowIndex="9">
                <Button fx:id="addNewClotheBtn" alignment="BOTTOM_LEFT" mnemonicParsing="false"
                        onAction="#addNewClothesBtn" text="Add New Clothes"/>
            </HBox>
            <Label fx:id="enterDataLblId" GridPane.rowIndex="9"/>
            <Label text="Serial Number" GridPane.rowIndex="1"/>
            <TextField fx:id="clothesSerialNumber" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        </GridPane>
    </content>
    <ButtonType fx:constant="CANCEL"/>
</DialogPane>
