<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<DialogPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea"
            fx:controller="com.example.store_app.Controllers.CustomElectronicsDialogBoxController">
    <content>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                  prefHeight="400.0" prefWidth="420.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0" prefWidth="117.99999237060544"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="462.00000762939453" minWidth="10.0" prefWidth="199.2"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="462.00000762939453" minWidth="10.0" prefWidth="106.0"/>

            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>

            </rowConstraints>
            <Text text="Name">
                <GridPane.margin>
                    <Insets left="10.0"/>
                </GridPane.margin>
            </Text>
            <Text text="Price" GridPane.rowIndex="1">

            </Text>
            <Text text="Image" GridPane.rowIndex="2">

            </Text>
            <TextField GridPane.columnIndex="1"/>
            <TextField prefHeight="26.0" prefWidth="169.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <TextField fx:id="electronicsImageTextField" editable="false" GridPane.columnIndex="1"
                       GridPane.rowIndex="2"/>
            <Button mnemonicParsing="false" onAction="#electronicsChooseImageBtnClicked" text="..."
                    GridPane.columnIndex="2" GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets left="20.0"/>
                </GridPane.margin>
            </Button>
            <Text text="Quantity" GridPane.rowIndex="3">

            </Text>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="6"/>
            <Text text="Manufacturer" GridPane.rowIndex="4">

            </Text>
            <Text wrappingWidth="115.0" text="Year of Production" GridPane.rowIndex="5">

            </Text>
            <Text text="Type" GridPane.rowIndex="6">

            </Text>
            <HBox alignment="BOTTOM_CENTER" GridPane.columnIndex="1" GridPane.rowIndex="7">
                <Button alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#addNewElectronicsBtn"
                        text="Add New Electronics"/>
            </HBox>
        </GridPane>
    </content>
    <ButtonType fx:constant="CANCEL"/>
</DialogPane>
